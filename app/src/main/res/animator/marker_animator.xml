<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <set>
            <objectAnimator android:propertyName="translationY" android:valueFrom="0" android:valueTo="-100" android:duration="200"/>
        </set>
    </item>
    <item android:state_pressed="false" >
        <set>
            <objectAnimator android:propertyName="translationY" android:valueFrom="-100" android:valueTo="0" android:duration="200"/>
        </set>
    </item>
</selector>